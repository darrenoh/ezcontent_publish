{
  "name": "srijanone/ezcontent_publish",
  "description": "A feature rich module enhancing the experience for Publishers.",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "homepage": "https://github.com/srijanone/ezcontent_publish",
  "support": {
    "issues": "https://github.com/srijanone/ezcontent_publish"
  },
  "repositories": {
    "ckeditor.colorbutton": {
      "type": "package",
      "package": {
        "name": "ckeditor/colorbutton",
        "version": "4.14.0",
        "type": "drupal-library",
        "extra": {
          "installer-name": "ckeditor.colorbutton"
        },
        "dist": {
          "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.14.1.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    },
    "ckeditor.panelbutton": {
      "type": "package",
      "package": {
        "name": "ckeditor/panelbutton",
        "version": "4.14.0",
        "type": "drupal-library",
        "extra": {
          "installer-name": "ckeditor.panelbutton"
        },
        "dist": {
          "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.14.1.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    },
    "ckeditor.preview": {
      "type": "package",
      "package": {
        "name": "ckeditor/preview",
        "version": "4.14.0",
        "type": "drupal-library",
        "extra": {
          "installer-name": "ckeditor.preview"
        },
        "dist": {
          "url": "https://download.ckeditor.com/preview/releases/preview_4.14.1.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    }
  },
  "require": {
    "drupal/colorbutton": "^1.2",
    "drupal/panelbutton": "^1.3",
    "drupal/ckeditor_layout_manager": "^1.0",
    "ckeditor/colorbutton":  "*",
    "ckeditor/panelbutton":  "*",
    "ckeditor/preview":  "*"
  }
}
