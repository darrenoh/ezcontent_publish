{
  "name": "srijanone/ezcontent_publish",
  "description": "A feature rich module enhancing the experience for Publishers.",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "homepage": "https://github.com/srijanone/ezcontent_publish",
  "support": {
    "issues": "https://github.com/srijanone/ezcontent_publish"
  },
  "require": {
    "drupal/colorbutton": "^1.2",
    "drupal/panelbutton": "^1.3",
    "drupal/ckeditor_layout_manager": "^1.0",
    "ckeditor-plugin/colorbutton": "4.14.1",
    "ckeditor-plugin/panelbutton": "4.14.1",
    "ckeditor-plugin/preview": "4.14.1"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "ckeditor-plugin/colorbutton",
        "version": "4.14.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.14.1.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor-plugin/panelbutton",
        "version": "4.14.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.14.1.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor-plugin/preview",
        "version": "4.14.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/preview/releases/preview_4.14.1.zip",
          "type": "zip"
        }
      }
    }
  ],
  "installer-paths": {
    "docroot/libraries/{$name}": ["vendor:ckeditor-plugin"]
  }
}
